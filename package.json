{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng lint --fix && ng test && ng build --configuration=production --sourceMap=false --extractCss=true --namedChunks=false",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverge": "jest --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:watch": "nodemon --exec \"npm run lint || exit 1\" --ext ts,html,scss"
  },
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@emailjs/browser": "^3.6.2",
    "angular-svg-icon": "^13.0.0",
    "ng-recaptcha": "^11.0.0",
    "ngx-page-scroll": "^10.0.0",
    "ngx-page-scroll-core": "^10.0.0",
    "normalize.css": "^8.0.1",
    "placeholder-loading": "^0.6.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@types/jest": "^29.5.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "jest-preset-angular": "^13.1.1",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.9.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup"
  }
}
